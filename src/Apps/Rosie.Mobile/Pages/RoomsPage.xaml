<?xml version="1.0" encoding="UTF-8"?>
<r:BasePage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" xmlns:r="clr-namespace:Rosie.Mobile;" x:Class="Rosie.Mobile.RoomsPage" Title="Rooms">
	<ListView ItemsSource="{Binding Rooms}" IsGroupingEnabled="true" GroupDisplayBinding="{Binding Key}">
		<ListView.ItemTemplate>
			<DataTemplate>
				<TextCell Text="{Binding Name}" Detail="{Binding Id}" />
			</DataTemplate>
		</ListView.ItemTemplate>
	</ListView>
</r:BasePage>