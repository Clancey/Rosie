<?xml version="1.0" encoding="UTF-8"?>
<r:BasePage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:r="clr-namespace:Rosie.Mobile;"  xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="Rosie.Mobile.DevicesPage" Title="Devices">
	<ListView ItemsSource="{Binding Devices}">
		<ListView.ItemTemplate>
			<DataTemplate>
				<TextCell Text="{ Binding Name }" Detail="{ Binding Id }" />
			</DataTemplate>
		</ListView.ItemTemplate>
	</ListView>
</r:BasePage>